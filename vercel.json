{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "env": {
    "PUBLIC_URL": "",
    "BASENAME": ""
  },

  "redirects": [
    {
      "source": "/rab2025",
      "destination": "https://docs.google.com/spreadsheets/d/1uhr4TSgSZXGfU7MjAW2r5L9os-ZAS6qm9/edit?gid=1417640007#gid=1417640007",
      "permanent": true
    }
  ],

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
