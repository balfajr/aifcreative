{
  "name": "aifcreative",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "config": {
    "githubBase": "/aif",
    "githubBasename": "/aif"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development PUBLIC_URL=/ BASENAME= webpack serve --config webpack.config.js --mode development",
    "build": "cross-env NODE_ENV=production PUBLIC_URL=/ BASENAME= webpack --config webpack.config.js --mode production",
    "preview": "cross-env NODE_ENV=production PUBLIC_URL=/ BASENAME= webpack serve --config webpack.config.js --mode production",
    "build:gh": "cross-env-shell NODE_ENV=production PUBLIC_URL=$npm_package_config_githubBase BASENAME=$npm_package_config_githubBasename webpack --config webpack.config.js --mode production",
    "deploy": "npm run build:gh && gh-pages -d dist"
  },
  "dependencies": {
    "@fontsource/plus-jakarta-sans": "^5.1.1",
    "axios": "^1.10.0",
    "framer-motion": "^12.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.15.1",
    "react-router-dom": "^7.6.2",
    "tailwindcss": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "@eslint/js": "^9.19.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.15",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "cross-env": "^7.0.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
